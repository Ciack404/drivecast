<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repo.drivecast"
        name="DriveCast repository"
        version="0.1"
        provider-name="InRete">
    </requires>
    	<import addon="script.module.simplejson" version="2.0.10"/>
    </requires>
    <extension point="xbmc.addon.repository"
        name="DriveCast Add-on Repository">
        <info>http://drivecast.googlecode.com/svn/trunk/addons.xml</info>
        <checksum></checksum>
        <datadir zip="false">http://drivecast.googlecode.com/svn/trunk</datadir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>DriveCast Add-ons for XBMC</summary>
        <description>Install DriveCast Repo Addons</description>
        <disclaimer></disclaimer>
        <platform>all</platform>
    </extension>
</addon>
</addons>
